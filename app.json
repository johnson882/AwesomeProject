{
  "expo": {

    
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],


    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
        "bundleIdentifier": "org.udacifitness.exp"
    },

    "android": {
      "package": "org.udacifitness.exp"
    },

    "orientation": "portrait",
    "name": "UdaciFitness",
    "description": "The best triathlon tracker ever",
    "slug": "udacfitness",
    "version": "1.0",
    "icon": "https://tylermcginnis.com/udacifitness-512.png",
    "notification": {
      "icon": "https://tylermcginnis.com/udacifitness-45.png"
    },
  }
}
